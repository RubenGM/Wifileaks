<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:padding="3dip">
	<TextView android:layout_height="wrap_content" android:text="@+id/TextView01"
		android:textStyle="bold" android:layout_width="wrap_content"
		android:id="@+id/tvSsid" android:textSize="18dip"></TextView>
	<TextView android:text="@+id/TextView02" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/tvBssid"></TextView>
	<TextView android:text="@+id/TextView02" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/tvCapabilities"></TextView>
	<TextView android:text="@+id/TextView03" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/tvKey"
		android:textSize="17dip" android:textColor="#FFF" android:shadowColor="#666"
		android:shadowDx="2" android:shadowDy="2" android:shadowRadius="2"></TextView>
	<ProgressBar android:max="100" android:progress="1"
		style="?android:attr/progressBarStyleHorizontal"
		android:layout_height="wrap_content" android:id="@+id/val"
		android:layout_width="match_parent"></ProgressBar>
</LinearLayout>
